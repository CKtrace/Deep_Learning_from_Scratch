    "import numpy as np\n",
    "import matplotlib.pylab as plt\n",
    "\n",
    "def stepfunc(x):\n",
    "    print(np.array(x > 0))\n",
    "    print(np.array(x > 0, dtype=np.int64))\n",
    "    return np.array(x > 0, dtype=np.int64)\n",
    "\n",
    "x = np.arange(-10.0, 10.0, 0.2)\n",
    "y = stepfunc(x)\n",
    "plt.plot(x, y)\n",
    "plt.ylim(-0.1, 1.1)\n",
    "plt.show()"
   ]
