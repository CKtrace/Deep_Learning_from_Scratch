    "import numpy as np\n",
    "import matplotlib.pylab as plt\n",
    "\n",
    "def relufunc(x):\n",
    "    return np.maximum(0,x)\n",
    "\n",
    "x = np.arange(-10.0, 10.0, 0.2)\n",
    "y = relufunc(x)\n",
    "plt.plot(x, y)\n",
    "plt.ylim(-0.1, 10)\n",
    "plt.show()"
   ]
  }
